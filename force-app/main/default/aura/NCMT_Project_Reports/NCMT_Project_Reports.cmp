                            <td data-label="w/ Subcontactor">
                                <div class="slds-truncate slds-text total-div" >
                                    <lightning:formattedNumber value="{!v.markup.Total_Cost_w_Subcon_Markups__c}" style="currency" currencyCode="USD"/>
                                </div>
                            </td>
                            <aura:if isTrue="{!v.isOandP}">
                                <td></td>
                            </aura:if>    
                            <td data-label="total">
