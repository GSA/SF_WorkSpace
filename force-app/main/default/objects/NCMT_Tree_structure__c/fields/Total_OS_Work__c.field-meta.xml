<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_OS_Work__c</fullName>
    <externalId>false</externalId>
    <formula>Total_Items_Cost__c
*(1 + Markup__r.Site_Design_Contingency__c)
*(1 + Markup__r.Prime_Con_OSWork_Homeoff_Overhead__c + Markup__r.Prime_Con_OSWork_Jobsite_Overhead__c)
*(1 + Markup__r.Prime_Con_OSWork_Profit__c)
*(1 + Markup__r.Prime_Con_OSWork_Commission__c)
*(1 + Markup__r.Prime_Con_OSWork_Bond__c)
*(1 + Markup__r.Professional_Services_Fees_OSWork__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total OS Work</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
