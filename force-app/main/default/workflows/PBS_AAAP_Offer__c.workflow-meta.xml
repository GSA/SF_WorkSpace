<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>PBS_AAAP_sendGrexSubmissionConfirmation</fullName>
        <description>PBS_AAAP_sendGrexSubmissionConfirmation</description>
        <protected>false</protected>
        <recipients>
            <field>PBS_AAAP_Offeror_Email__c</field>
            <type>email</type>
        </recipients>
        <senderAddress>lop.manager@gsa.gov</senderAddress>
        <senderType>OrgWideEmailAddress</senderType>
        <template>unfiled$public/PBS_AAAP_sendGrexSubmissionConfirmation</template>
    </alerts>
    <alerts>
        <fullName>PBS_AAAP_sendGrexSubmissionFailure</fullName>
        <description>PBS_AAAP_sendGrexSubmissionFailure</description>
        <protected>false</protected>
        <recipients>
            <field>PBS_AAAP_Offeror_Email__c</field>
            <type>email</type>
        </recipients>
        <senderAddress>lop.manager@gsa.gov</senderAddress>
        <senderType>OrgWideEmailAddress</senderType>
        <template>unfiled$public/PBS_AAAP_sendGrexSubmissionFailure</template>
    </alerts>
    <alerts>
        <fullName>PBS_AAAP_sendGrexSubmissionFailuretoGREX</fullName>
        <ccEmails>RSAP.Manager@gsa.gov</ccEmails>
        <description>PBS_AAAP_sendGrexSubmissionFailuretoGREX</description>
        <protected>false</protected>
        <senderAddress>lop.manager@gsa.gov</senderAddress>
        <senderType>OrgWideEmailAddress</senderType>
        <template>unfiled$public/PBS_AAAP_sendGrexSubmissionFailuretoGREX</template>
    </alerts>
    <rules>
        <fullName>HubZone Identifier</fullName>
        <actions>
            <name>Update_Hubzone_Identifier</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND 2</booleanFilter>
        <criteriaItems>
            <field>PBS_AAAP_Offer__c.PBS_AAAP_HubZone_SBC__c</field>
            <operation>equals</operation>
            <value>Yes</value>
        </criteriaItems>
        <criteriaItems>
            <field>PBS_AAAP_Offer__c.PBS_AAAP_HubZone_SBC_Preference__c</field>
            <operation>equals</operation>
            <value>No</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>HubZone Identifier Revert</fullName>
        <actions>
            <name>HubZone_Identifier_Revert</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>NOT(1 AND 2)</booleanFilter>
        <criteriaItems>
            <field>PBS_AAAP_Offer__c.PBS_AAAP_HubZone_SBC__c</field>
            <operation>equals</operation>
            <value>Yes</value>
        </criteriaItems>
        <criteriaItems>
            <field>PBS_AAAP_Offer__c.PBS_AAAP_HubZone_SBC_Preference__c</field>
            <operation>equals</operation>
            <value>No</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>PBS_AAAP_sendGrexSubmissionConfirmation</fullName>
        <actions>
            <name>PBS_AAAP_sendGrexSubmissionConfirmation</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>PBS_AAAP_sendGrexSubmissionConfirmation</description>
        <formula>AND(PBS_AAAP_Status_Code__c != null, PBS_AAAP_Status_Code__c == &apos;200&apos;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>PBS_AAAP_sendGrexSubmissionFailure</fullName>
        <actions>
            <name>PBS_AAAP_sendGrexSubmissionFailure</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(PBS_AAAP_Status_Code__c != null, PBS_AAAP_Status_Code__c != &apos;200&apos;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>PBS_AAAP_sendGrexSubmissionFailuretoGREX</fullName>
        <actions>
            <name>PBS_AAAP_sendGrexSubmissionFailuretoGREX</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(PBS_AAAP_Status_Code__c != null, PBS_AAAP_Status_Code__c != &apos;200&apos;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
