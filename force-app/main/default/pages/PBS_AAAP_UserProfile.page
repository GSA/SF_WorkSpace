    border:1px solid #999;
}
 
.ui-dialog a {
    float:right;
    font-weight:bold;
    padding:3px;
    color:#999;
    text-transform:uppercase;
    font-size:10px;
} 

#lean_overlay {
    position: fixed;
    z-index:100;
    top: 0px;
    left: 0px;
    height:100%;
    width:100%;
    background:none;
    display: none;
}

.errMessage {color:red; }
#nav ul {
