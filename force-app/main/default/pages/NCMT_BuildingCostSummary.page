        function show_hide_column(col_no, do_show) {
            var tbl = document.getElementById('ReportData');
            var rows = tbl.getElementsByTagName('tr');
            
            for (var row = 0; row < rows.length; row++) {
                var cols = rows[row].children;
                if (col_no >= 0 && col_no < cols.length) {
                    var cell = cols[col_no];
                    if (cell.tagName == 'TD') cell.style.display = do_show ? 'block' : 'none';
                }
            }
        }
        
        function get(name){
            if(name=(new RegExp('[?&]'+encodeURIComponent(name)+'=([^&]*)')).exec(location.search))
                return decodeURIComponent(name[1]);
        }
        
        function DecimalToDollar(Amount) {
            Amount = parseFloat(Amount).toFixed(2);
            return '$'+ Amount.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        }
        
        function DecimalToDollar0(Amount) {
            Amount = parseFloat(Amount).toFixed(0);
            return '$'+ Amount.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        }
        
        function DollarToDecimal(Amount){
            Amount = Amount.replace('$','');
            Amount = Amount.replace('$','').replace(',','').replace(',','').replace(',','').replace(' ',''); // Easy way to make sure that we handle big numbers, or more commas ...
            return Amount;      
        }
        
