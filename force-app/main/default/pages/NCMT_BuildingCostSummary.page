<apex:page Controller="NCMT_ReportData" title="{!strCUIHeader}" readonly="true" showHeader="false" sidebar="false" cache="true" lightningStylesheets="true">
    <head>
        <style>        
            table {
            border-collapse: collapse; 
            }
            .borderR {
            border-Right: 1px solid;
            }
            .topBorder {
            border-top: 1px solid;
            }
            .bottomBorder {
            border-bottom: 1px solid;
            }
            .borderL {
            border-left: 1px solid;
            }
            .number {
            font-size: 12px;
            border-bottom: 1px solid;
            }
            
            @media print {
                .print-margin {margin-top:75px !important;}
                .screen-only { display: none !important; }
                
            @page { 
                    size: landscape !important;                    
                    font-size: 9px !important;                    
                }            
            }
            
            div.footer {
            display: block;
            padding: 1px;
            position: running(footer);
            }
            #ReportData td, tr{
            padding:.1%;
            }
            .screen-only {
              font-size: 1.5rem;
              color: darkred;
              text-align: center;
            }
            
        </style>
    </head>     
    <apex:form id="rptUniformat">
        <div class="screen-only">
            {!strCUIHeader}
        </div>
        <div class="footer"></div>
