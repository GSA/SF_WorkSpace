<apex:page controller="PBS_gBUILD_PBuildingScopeSysController" sidebar="false" tabStyle="PBS_gBUILD_PBuildingScope__tab">
    <c:PBS_gBUILD_InitializeScripts />  
    <apex:form >
        <apex:sectionHeader title="Scope Detail: {!sectionTitle}" />
        <apex:pageMessages />           
        <c:PBS_gBUILD_PageInstructions html="{!scopeInfo.detailPageIntro}" />
        <apex:outputPanel rendered="{!showPageContent}" >        
        <apex:pageBlock title="Step 2: {!if(scopeInfo.detailPageDetailTitle==null,'Details', scopeInfo.detailPageDetailTitle)}" >
            <apex:pageBlockButtons >
                <apex:commandButton action="{!edit}" value="Edit" rendered="{!viewMode}" disabled="{!readonlyMode}" />
                <apex:commandButton action="{!descope}" value="Descope" rendered="{!editMode}" disabled="{!readonlyMode}" />
                <apex:commandButton action="{!save}" value="Save" rendered="{!editMode}" disabled="{!readonlyMode}" />
                <apex:commandButton action="{!cancel}" value="Cancel" rendered="{!editMode}" immediate="true" />
                <apex:commandButton action="{!approve}" value="Approve"  rendered="{!viewMode}" disabled="{!!isApproveEnabled}" styleClass="approve"/>
            </apex:pageBlockButtons>        
            <apex:outputPanel rendered="{!(scopeInfo.detailPageDetailIntro!=null)}">
                <div class="message">
                  <!--  <apex:outputText value="{!scopeInfo.detailPageDetailIntro}" escape="false" />-->
                  <!-- Replaced line 18 with line 20 for story PBS- 1701-->
                  <apex:outputText value="{{!JSENCODE(htmlEncode(scopeInfo.detailPageDetailIntro))}}" escape="false" />  
                </div>
            </apex:outputPanel>                                    
            <div style="display:none" id="descopeDiv">
                <apex:inputField value="{!scopeSystem.DescopingReason__c}" />
            </div>
            <div id="dialog-form" title="De-Scope this item" style="background-color:white;">
                <fieldset>
                    <label for="jsDescopingReason">Reason</label>
                    <input type="text" name="jsDescopingReason" id="jsDescopingReason" class="text ui-widget-content ui-corner-all" style="width:100%" />
                </fieldset>
            </div>                      
            <script type="text/javascript">
                var executeDescope = false;
                var descopeButton = j$("input[type='submit'][value='Descope']");
                j$("#dialog-form").dialog({
                    autoOpen: false,
                    height: 200,
                    width: 400,
                    modal: true,
                    buttons: {
                        "De-Scope": function () {
                            var reason = j$("#jsDescopingReason").val();
                            //alert(reason);
                            executeDescope = true;
                            j$("#descopeDiv textarea").val(reason);
                            j$(this).dialog("close");
                            descopeButton.trigger("click");
                        },
                        Cancel: function () {
                            //alert("CANCEL");
                            executeDescope = false;
                            j$(this).dialog("close");
                        }
                    },
                    close: function () {
                    }
                });
                descopeButton.click(function () {
                    if (executeDescope) {
                        executeDescope = false;
                        //alert("tiggered");return false;
                        return true;
                    }
                    j$("#dialog-form").dialog("open");
                    return false;
                });
            </script>                       
            <apex:pageBlockSection title="Basic Tracking Information" columns="2">
