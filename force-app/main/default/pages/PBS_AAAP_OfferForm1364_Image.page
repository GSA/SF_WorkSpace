                                                                                                                        }
                                                                                                                    });
                                                                                                        return false;
                                                                                                    }
    
    
    function escapeRegExp(string){
        return string.replace(/[.*+?^${}()|[\]\\]/g, "\\$&");
    }
    
    function replaceAll(str, term, replacement) {
        return str.replace(new RegExp(escapeRegExp(term), 'g'), replacement);
    }
    
    
    function captureDone(){
        debugger;
        
        var recId = '{!JSENCODE($CurrentPage.parameters.offerId)}';   
        var total = '{!JSENCODE($CurrentPage.parameters.total)}';  
        var current = '{!JSENCODE($CurrentPage.parameters.current)}'; 
        
        var agid = '';
        var rlpyear ;
        var history = '';
        current = parseInt(current)+1;
        
        decodedCookie = {!JSENCODE('document.cookie')};
        var ca = decodedCookie.split(';');
        for(var j = 0; j <ca.length; j++) {
            name = ca[j].split('=')[0];
            value = ca[j].split('=')[1];
            if(name.trim() == 'apex__links'){
                ids = value;
            }
            
            if(name.trim() == 'apex__agencyId'){
                agid = value;
            }
            
            if(name.trim() == 'apex__RLPYear'){
                rlpyear = value;
            }
            
            if(name.trim() == 'apex__histId'){
                history = value;
            }
        }           
        
        //alert(rlpyear);
        var rlpYearInt = 99999;
        if(rlpyear != ''
           && rlpyear != undefined
           && rlpyear != null){
            rlpYearInt = parseInt(rlpyear);
            
            if(rlpYearInt >= 2019){
                //openNexForm();
                // checking if we have ids in cookie
                if(ids != ''){
                    var allIds = ids.split(',');
                    var firstEle = allIds[0];
                    allIds.shift();
                    
                    var stringArray = allIds.join();
                    document.cookie = "apex__links="+stringArray;                           
                    var newurl = replaceAll(firstEle, '#', '=')
                    window.location.href = newurl+'&current='+current+'&total='+total;
                }
                else{
                    //alert(agid);
                    window.location.href = "/apex/PBS_AAAP_ImagePDF?agencyId="+agid+'&histId='+history+'&isImagePrintPage=yes';   
                }
            }
        }
        
        if(rlpYearInt < 2019){
            // checking if we have ids in cookie
            if(ids != ''){
                var allIds = ids.split(',');
                var firstEle = allIds[0];
                allIds.shift();
                
                var stringArray = allIds.join();
                document.cookie = "apex__links="+stringArray;                           
                var newurl = replaceAll(firstEle, '#', '=')
                window.location.href = newurl+'&current='+current+'&total='+total;
            }
            else{
                //alert(agid);
                window.location.href = "/apex/PBS_AAAP_ImagePDF?agencyId="+agid+'&histId='+history+'&isImagePrintPage=yes';   
            }
        }
    }
    
    
